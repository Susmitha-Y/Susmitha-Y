#Hello World
print("Hello World")

#Hello World with tab
print("hello world \t hello world")

#Hello World with new line
print("hello world \n hello world")

#Integer Input
a=int(input())
print("%d"%a)

#Float Input
a=float(input())
print("%.2f"%a)

#Character Input
a=input()
print(a)

#String Input
a=input()
print(a)

#Ascii-I
a=input()
print(ord(a))

#Ascii-II
a=int(input())
print(chr(a))
